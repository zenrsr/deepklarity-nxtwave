APP_NAME=ai-quiz-backend
APP_ENV=development
APP_HOST=0.0.0.0
APP_PORT=8000
LOG_LEVEL=INFO
ALLOWED_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Database configuration (PostgreSQL + asyncpg)
# Example local/dev connection strings:
#   postgresql+asyncpg://postgres:postgres@localhost:5432/aiquiz
#   postgresql+asyncpg://postgres:postgres@db:5432/aiquiz   (inside docker-compose)

# Replace with your production DSN before deployment
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5433/aiquiz

# GOOGLE GEMINI AI
# Set this to your Gemini API key (never commit actual secrets)
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
GEMINI_MODEL=models/gemini-2.5-flash

# Scraper settings
SCRAPER_MAX_CONCURRENCY=2
REQUEST_TIMEOUT_SECONDS=15
EOF
